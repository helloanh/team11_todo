<div class="container">
<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h1>Create a New Todo List</h1>
		<%= form_for @to_do_list do |f| %>

		  <%= label_tag(:list_name, "List Name:") %>
		  <%= f.text_field :list_name %></br>

		  <%= label_tag(:list_due_date, "List Due Date:") %> 
		  <%= f.date_field :list_due_date, class: 'form-control' %></br>
		  <%= f.submit "Create Todo List" %>
		<% end %>

    </div>
  </div>
</div>